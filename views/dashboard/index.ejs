		<link rel="stylesheet" href="/bootstrap/dist/css/bootstrap-theme.min.css" media="screen" title="no title" charset="utf-8">
		<link rel="stylesheet" href="/bootstrap/dist/css/bootstrap-theme.css" media="screen" title="no title" charset="utf-8">
    <link rel="stylesheet" href="/css/dashboard.css" media="screen" title="no title" charset="utf-8">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:300&subset=latin,greek' rel='stylesheet' type='text/css'>
  </head>
  <body class="co">
    <div class="page">
      <div class="navbar-nb">
        <ul>
          <li class="brand"><a href="#">Jump To:</a></li>
          <li><a href="#stats">Stats</a></li>
          <li><a href="news.asp">News</a></li>
          <li><a href="contact.asp">Contact</a></li>
          <li><a href="about.asp">About</a></li>
          <li class="seli">
            <a class="se">
              <form action="/dashboard" method="get">
                <input class="search" type="text" placeholder="Search..." required name="search">
                <input class="button" type="submit" value="Search">
              </form>
            </a>
          </li>
        </ul>
      </div>
      <div class="project-head">
        <h1><%=sname%></h1>
        <ul>
          <li><a href="#new-web-app"><button type="button" class="btn btn-default"><span class="glyphicon glyphicon-plus"></span> New web-app</button></a></li>
        </ul>
      </div>
    </div>
    <div class="sidebar">
      <!-- Sidebar -->
      <div class="welcome">
        <img src="<%=imgprofile%>" alt="" id="wimg"/>
        <div class="wtext">
          <h5>Welcome Back,</h5>
          <h4>Admin</h4>
        </div>
      </div>
      <ul id="sidebar1">
        <a href="/dashboard"><li class="ctive sidebar-li"><div class="active-bar"></div><h5><span class="glyphicon glyphicon-dashboard"></span> Dashboard</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-th"></span> Apps</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-cloud"></span> Files</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-user"></span> Users</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-signal"></span> Servers</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-compressed"></span> Packages</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-chevron-right">__</span> Terminal</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-refresh"></span> Updates</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-cog"></span> Settings</h5></li></a>
        <a id="sidebara" href="/signout"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-log-out"></span> Sign Out</h5></li></a>
        <a id="sidebara"><li class="sidebar-li"><h5><span class="glyphicon glyphicon-off"></span> Power</h5></li></a>
      </ul>
    </div>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <!-- Hamburger menu script for hamburger -->
    <script src="/js/hamburger.js"></script>
    <script src="/notie/notie.js" type="text/javascript"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script type="text/javascript">
      $(document).ready(function () {
        //notie.alert(1, 'Success!', 1.5);
        $('.carousel').carousel();
      });
      var socket = io.connect();
      socket.on('request', function(req){
        document.getElementById("req").innerHTML = req+" requests today";
      });
      socket.on('runtime', function(req){
        document.getElementById("wrun").innerHTML = "Been running for "+req+' minutes';
      });
      $(document).ready(function (){
            $(".ase").click(function (){
                $('html, body').animate({
                    scrollTop: $("#info").offset().top}, 800);
                $('nav').removeClass('navbar-fixed-top');
            });
            $('[data-toggle="tooltip"]').tooltip();
            $(".sidebar-li").click(function () {
              $(this).css('background-color', "#2c3e50");
            })
            $(".sidebara").mouseover(function () {
              $(this).css('background-color', "#2c3e50");
            })
            $(".sidebara").mouseout(function () {
              $(this).css('background-color', "#34495e");
            })
        });
    </script>
  </body>
</html>
