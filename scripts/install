#!/usr/env/node
var exec = require('child_process').exec;
var os = require('os');
var commander = require('commander');

// This is the Web-OS install script

// Get arguments
commander
  .version('0.0.1')
  .option('-l, --local', 'Installs all the apps in /packages/ using msiexec and adds it to the path for the server on startup (Only avalible on Windows)')
  .command()
  .parse(process.argv);

if(args === "[ 'h' ]"){
  console.log("ok");
}
